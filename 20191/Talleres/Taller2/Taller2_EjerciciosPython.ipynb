    "import numpy as np\n",
    "def MatrizFrecuencia(lis=[]):\n",
    "    col = lis[0]\n",
    "    col = len(col)\n",
    "    i=0\n",
    "    pos=0\n",
    "    conA=0\n",
    "    conC=0\n",
    "    conG=0\n",
    "    conT=0\n",
    "    matriz = np.zeros((4,col))\n",
    "    while (i<col):\n",
    "        while (pos<len(lis)):\n",
    "            cadena = lis[pos]\n",
    "            if (cadena[i]=='A'):\n",
    "                conA = conA + 1\n",
    "            elif (cadena[i]=='C'):\n",
    "                conC = conC + 1\n",
    "            elif (cadena[i]=='G'):\n",
    "                conG = conG + 1\n",
    "            elif (cadena[i]=='T'): \n",
    "                conT = conT + 1\n",
    "            pos = pos +1\n",
    "        matriz[0][i]=conA\n",
    "        matriz[1][i]=conC\n",
    "        matriz[2][i]=conG\n",
    "        matriz[3][i]=conT\n",
    "        i = i +1\n",
    "        pos=0\n",
    "        conA=0\n",
    "        conC=0\n",
    "        conG=0\n",
    "        conT=0\n",
    "    return matriz\n",
    "            \n",
    "        "
