  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "def distancia_Hamming(poliA,poliB):\n",
    "    if (len(poliA)==len(poliB)):\n",
    "        cont=0\n",
    "        i=0\n",
    "        while i<len(poliA):\n",
    "            if (poliA[i] != poliB[i]):\n",
    "                cont=cont+1\n",
    "            i=i+1\n",
    "    return cont\n"
   ]
